@model List<Product>
@{
    ViewData["Title"] = "List";
    int count = 1;
}

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Title</th>
            <th scope="col">Price</th>
            <th scope="col">Quantity</th>
            <th scope="col">Date</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in @Model)
        {
        <tr>
            <th scope="row">@(count++)</th>
            <td>@item.Title</td>
            <td>@item.Price</td>
            <td>@item.Quantity</td>
            <td>@item.Date</td>
        </tr>
        }

    </tbody>
</table>